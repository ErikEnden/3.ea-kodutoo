{
    "name":"SpotifySearchRealQuick",
    "version": "1.0",
    "description": "Spotify context menu search.",
    "manifest_version": 2,
    "options_page": "options.html",
    "background": {
        "scripts":["jquery-3.3.1.min.js","context.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_idle",
            "js": ["search.js"]
        }
    ],
    "permissions": [
        "contextMenus",
        "tabs",
        "identity",
        "storage"
    ]
}